# Environment variables used by the Expo web app
# Copy this file to .env and set values as appropriate for your environment.

# Public config (these are read at build/runtime by Expo Web)
EXPO_PUBLIC_LOG_LEVEL=info
EXPO_PUBLIC_FEATURE_FLAGS=
EXPO_PUBLIC_EXPERIMENTS_ENABLED=
EXPO_PUBLIC_TRUST_PROXY=true
EXPO_PUBLIC_HEALTHCHECK_PATH=/healthz
EXPO_PUBLIC_PORT=3000
